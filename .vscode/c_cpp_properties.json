{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/programs/zudi/include",
                "${workspaceFolder}/__kcore/include",
                "${workspaceFolder}/b/include",
                "${workspaceFolder}/**"
            ],
            "defines": [
                "__ZAMBESII_KERNEL_SOURCE__=1"
            ],
            "compilerPath": "/usr/bin/i686-linux-gnu-g++",
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "linux-gcc-x86",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/b"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "forcedInclude": [
                "${workspaceFolder}/b/include/config.h"
            ]
        }
    ],
    "version": 4
}
